{
    "$schema": "http://json-schema.org/draft-07/schema#",
    "$id": "http://item.com/itemNew.schema.json",
    "type": "object",
    "properties": {
      "name": {
        "type": "string",
        "minLength": 1,
        "maxLength": 50
      },
      "value": {
        "type": "number",
        "minimum": 0.00
      },
      "inStock": {
        "type": "boolean"
      }
    },
    "additionalProperties": false,
    "required": [
      "name",
      "value"
    ]
  }